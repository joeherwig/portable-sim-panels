    script(type='text/javascript').
      let config = {};
      let path = window.parent.location.pathname.split("/");
      path.pop();
      path = path.join("/") + "/";
      fetchAsync('config.json')
      .then(config => config)
      .catch(reason => {
        alert("Error in " + window.location + "config.json\n\n" + reason)
        console.log("Error in config.json:\n" + reason)
      })
      let json = {};
      window.parent.addEventListener("update", eventHandler, true);
      function eventHandler(update) {
          json = Object.assign(json, update.detail);
      }
