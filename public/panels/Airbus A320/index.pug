doctype html
html(onload="document.body.requestFullscreen();"  lang="en" xml:lang="en" xmlns="http://www.w3.org/1999/xhtml")
  head
    title Airbus A320 panel
    link(rel="import" href="../../libs/headerScripts.html")
    meta(name="mobile-web-app-capable" content="yes")
    meta(name="viewport" content="minimal-ui, width=device-width, initial-scale=1.0, user-scalable=yes")
    link(rel="stylesheet" href="panel.css")
    script(src='../../libs/d3.3.min.js')
    script(src='../../libs/portable-sim-panels.js')
    script(src='../../libs/findSuitablePanel.js')
    script(type='text/javascript').
      let config = {};
      let path = window.parent.location.pathname.split("/");
      path.pop();
      path = path.join("/") + "/";
      fetchAsync('config.json')
      .then(config => config)
      .catch(reason => {
        alert("Error in " + window.location + "config.json\n\n" + reason)
        console.log("Error in config.json:\n" + reason)
      })
      let json = {};
      window.parent.addEventListener("update", eventHandler, true);
      function eventHandler(update) {
          json = Object.assign(json, update.detail);
      }
